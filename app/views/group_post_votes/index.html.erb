<h1>Listing group_post_votes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>group post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_post_votes.each do |group_post_vote| %>
      <tr>
        <td><%= group_post_vote.user_id %></td>
        <td><%= group_post_vote.group_post_id %></td>
        <td><%= link_to 'Show', group_post_vote %></td>
        <td><%= link_to 'Edit', edit_group_post_vote_path(group_post_vote) %></td>
        <td><%= link_to 'Destroy', group_post_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New group post vote', new_group_post_vote_path %>
