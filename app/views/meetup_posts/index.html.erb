<h1>Listing meetup_posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Meetup</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetup_posts.each do |meetup_post| %>
      <tr>
        <td><%= meetup_post.title %></td>
        <td><%= meetup_post.description %></td>
        <td><%= meetup_post.meetup_id %></td>
        <td><%= meetup_post.user_id %></td>
        <td><%= link_to 'Show', meetup_post %></td>
        <td><%= link_to 'Edit', edit_meetup_post_path(meetup_post) %></td>
        <td><%= link_to 'Destroy', meetup_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meetup post', new_meetup_post_path %>
