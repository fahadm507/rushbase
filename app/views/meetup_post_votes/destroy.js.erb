var targetUpvoteForm = $("#upvote-btn-<%= @meetup_post_vote.meetup_post.id %>")
var upvoteTarget = $("#count-upvotes-<%= @meetup_post_vote.meetup_post.id %>")

targetUpvoteForm.html("<%= j(render partial: 'meetup_post_votes/meetup_post_vote',
  locals: {
    post: @meetup_post_vote.meetup_post,
    meetup: @meetup_post_vote.meetup_post.meetup
  })%>")

upvoteTarget.html("<%= j(render partial: 'meetup_post_votes/count_upvotes',
  locals: {post: @meetup_post_vote.meetup_post})%>");
