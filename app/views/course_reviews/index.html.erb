<h1>Listing course_reviews</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>User</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_reviews.each do |course_review| %>
      <tr>
        <td><%= course_review.title %></td>
        <td><%= course_review.description %></td>
        <td><%= course_review.user_id %></td>
        <td><%= course_review.course_id %></td>
        <td><%= link_to 'Show', course_review %></td>
        <td><%= link_to 'Edit', edit_course_review_path(course_review) %></td>
        <td><%= link_to 'Destroy', course_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course review', new_course_review_path %>
