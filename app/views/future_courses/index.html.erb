<h1>Listing future_courses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @future_courses.each do |future_course| %>
      <tr>
        <td><%= future_course.name %></td>
        <td><%= future_course.user_id %></td>
        <td><%= future_course.organization %></td>
        <td><%= link_to 'Show', future_course %></td>
        <td><%= link_to 'Edit', edit_future_course_path(future_course) %></td>
        <td><%= link_to 'Destroy', future_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Future course', new_future_course_path %>
