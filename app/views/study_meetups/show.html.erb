<div class="container">
  <div class="study-meetup-header row">
  <div class="col-md-4 col-sm-6 col-xs-12 meetup-stats" style="">
      <h3 class="text-left">45 members</h3>
      <h3 class="text-left">28 recommendations</h3>
      <h3 class="text-left">432 posts</h3>
    </div>
    <div class="meetup-banner-wrapper col-md-8 col-sm-12 col-xs-12 text-center">
      <div class=" meetup-banner">
        <h1 class="meetup-headline"><%= @study_meetup.name %></h1>
        <a href="" class="btn btn-default">Join</a>
      </div>
      <div class="meetup-actions">
          <%= render partial: 'users/user_profile_footer', locals: { user: current_user } %>
      </div>
    </div>

  </div>
</div>
<div class="container">
  <div class="row meetup-content">
