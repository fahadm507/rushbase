        <p>Meet other users</p>
        <div role="tabpanel">
        <!-- Nav tabs -->
          <ul class="nav nav-tabs" role="tablist">
            <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Posts</a></li>
            <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Users</a></li>
            <li role="presentation"><a href="#messages" aria-controls="messages" role="tab" data-toggle="tab">topics</a></li>
            <li role="presentation"><a href="#settings" aria-controls="settings" role="tab" data-toggle="tab">Settings</a></li>
          </ul>

          <div class="tab-content">
            <div role="tabpanel" class="tab-pane active" id="home">posts</div>
            <div role="tabpanel" class="tab-pane" id="profile">users</div>
            <div role="tabpanel" class="tab-pane" id="messages">favorites</div>
            <div role="tabpanel" class="tab-pane" id="settings">settings</div>
          </div>
        </div>
      </div>
    </div>
    <span></span>
    <span id="explore" style="display:none;" >adsfdsfs</span>
